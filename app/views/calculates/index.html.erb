<h1>Listing calculates</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calculates.each do |calculate| %>
      <tr>
        <td><%= calculate.product_id %></td>
        <td><%= calculate.qty %></td>
        <td><%= link_to 'Show', calculate %></td>
        <td><%= link_to 'Edit', edit_calculate_path(calculate) %></td>
        <td><%= link_to 'Destroy', calculate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calculate', new_calculate_path %>
